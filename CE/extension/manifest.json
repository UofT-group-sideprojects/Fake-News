{

	"manifest_version": 2,
	"name": "Grain O' Salt",
	"description": "Analyze the trustworthiness of the News Article you are on",
	"version": "1.0",

	"browser_action": 
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},

	"permissions": [
		"tabs",
		"activeTab",
		"https://ajax.googleapis.com/"
	],
	"sockets": { 
		"tcp": { 
			"connect" : "*:*" 
		},
		"udp": {
      		"send": "*" 
	} 
}