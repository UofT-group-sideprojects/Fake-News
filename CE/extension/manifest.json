{

	"manifest_version": 2,
	"name": "True News",
	"description": "Analyze the trustworthiness of the News Article you are on",
	"version": "1.0",

	"browser_action": 
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"Analyze":{
		"persistent": true

	},

	"permissions": [
		"tabs",
		"activeTab",
		"socket.io",
		"https://ajax.googleapis.com/",
		"experimental",
    		{"socket": [
        		"udp-send-to"
    		]},
    	"app.window"
	]
}